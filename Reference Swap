# include<iostream>
using namespace std;
void swap(int *a,int *b)
{
    int k = *a;
    *a=*b;
    *b=k;
    
    
}
int main()
{
    int a,b;
    cout<<"First Number"<<endl;
    cin>>a;
    cout<<"Second Number"<<endl;
    cin>>b;
    swap(a,b);
    cout<<"First Number "<<a;
    cout<<""<<endl;
    cout<<"Second Number "<<b;
    
}
