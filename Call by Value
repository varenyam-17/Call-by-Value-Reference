# include<iostream>
using namespace std;
void swap(int a,int b)
{
    int temp=a;
    a=b;
    b=temp;
    cout<<"First Number "<<a;
    cout<<""<<endl;
    cout<<"Second Number "<<b;
}
int main()
{
    int a,b;
    cout<<"First Number"<<endl;
    cin>>a;
    cout<<"Second Number"<<endl;
    cin>>b;
    swap(a,b);
    
}
